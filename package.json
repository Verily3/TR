{
  "name": "transformation-os",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "db:generate": "pnpm --filter @tr/db db:generate",
    "db:migrate": "pnpm --filter @tr/db db:migrate",
    "db:seed": "pnpm --filter @tr/db db:seed",
    "db:studio": "pnpm --filter @tr/db db:studio"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  }
}
